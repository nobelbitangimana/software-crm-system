{
  "name": "crm-frontend",
  "version": 2,
  "builds": [
    {
      "src": "build/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/build/$1"
    },
    {
      "src": "/.*",
      "dest": "/build/index.html"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "https://crm-backend-jet-seven.vercel.app/api"
  }
}